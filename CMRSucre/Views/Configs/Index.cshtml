@model IEnumerable<CMRSucre.Models.Config>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Utilidad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fecha_Cierre)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Utilidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha_Cierre)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ConfigId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ConfigId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ConfigId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
